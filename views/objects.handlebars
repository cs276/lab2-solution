<h1>Object Listing</h1>
<h2>Gallery {{gallery}}</h2>
<table class='table table-striped'>
  {{#each objects}}
    <tr>
      <td>
        <a href="/object/{{this.id}}">
          {{this.title}}
        </a>
      </td>
      <td><img class='gallery-image' src="{{this.primaryimageurl}}"></img></td>
      <td><a href="{{object.url}}">More Information</a></td>
    </tr>
  {{/each}}
</table>
